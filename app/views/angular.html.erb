                                <a href=""><span class="glyphicon glyphicon-ok"></a></span>&nbsp;&nbsp;&nbsp;
                                <a href="#" onclick="getConfirmation();"><span class="glyphicon glyphicon-trash"></span></a></p>
                              <form action="" method="post">
                                  <div class="inputContainer">
                                      <input type="text" id="name"
                                             class="taskName"
                                             placeholder="Name"
                                             ng-model="newTask">
                                  </div>
                                  <div class="inputContainer">
                                      <textarea
                                              id="description"
                                              class="taskDescription"
                                              placeholder="Description"
                                              ng-model="newTask"
                                              rows="3"
                                              cols="40"></textarea>
                                      <label for="description"></label>
                                  </div>
                                  <div class="inputContainer">
                                      <label style="padding-left:20px;">Date effective</label>
                                      <input type="date"
                                             id="dateeffective"
                                             class="taskDate"
                                             ng-model="newTaskDate">
                                      <label>Date due</label>
                                      <input type="date"
                                             id="duedate"
                                             class="taskDate"
                                             ng-model="newTaskDate">
                                  </div>

                                  <div class="inputContainer">
                                      <input type="text"
                                             id="create_by"
                                             class="createBy"
                                             placeholder="Created by"
                                             ng-model="create_by">
                                  </div>
                                  <hr/>
                                  <div class ="priority">
                                      <select>
                                        <option value="high Priority">High Priority</option>
                                        <option value ="low priority">Low Priority</option>
                                      </select>
                                  </div>
                                  <!-- <div class ="add">
                                    <i class="fa fa-plus fa-2x"></i>
                                  </div> -->
                              </form>
                      </div>
                      <!-- <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                      </div> -->
                    </div>
                  </div>
                </div>
              </div>
              </div>
